[
  {
    "type": "string",
    "primitive": "string"
  },
  {
    "type": "integer",
    "primitive": "integer",
    "regex": "^([0-9]+)$"
  },
  {
    "type": "number",
    "primitive": "number",
    "regex": "^([+-]?[0-9]+(\\.[0-9]+)?)$"
  },
  {
    "type": "boolean",
    "primitive": "boolean",
    "values": ["true","false"]
  },
  {
    "type": "alpha",
    "is": "string",
    "regex": "^([a-zA-Z]+)$"
  },
  {
    "type": "alnum",
    "is": "string",
    "regex": "^([a-zA-Z][a-zA-Z0-9]*)$"
  }
]